alias gname="git config --local user.name"
alias gemail="git config --local user.email"
alias glocal="git config --list --local"
alias gc="git checkout"
alias gl="git log"
alias gcb="git checkout -b"
alias gcd="git checkout development && git pull origin development"
alias gcstg="git checkout staging && git pull origin staging"
alias gcm="git checkout main && git pull origin main"
alias gf="git fetch"
alias gcl="git clone"
alias gd="git diff"
alias gm="git merge"
alias gp="git pull origin"
alias gps="git push origin"
alias gst="git status"
alias gbl="git branch -l"
alias gba="git branch -a"
alias gbr="git branch -r"
alias	gbm="git branch -m"
alias gbd="git branch -d"
alias ga="git add ."
alias gacm="git add . && git commit -m"
alias gabc="git add . && better-commits"
alias gbc="better-commits"
alias gbb="better-branch"
alias gasr="git config --local --add --bool push.autoSetupRemote true"